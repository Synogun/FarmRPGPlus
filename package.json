{
    "name": "farmrpg-plus",
    "symbol": "FRPGP",
    "version": "1.8.2",
    "description": "Add features to Farm RPG while keeping code of conduct in mind.",
    "private": true,
    "scripts": {
        "utils:test-vault": "vitest --run",
        "remove:dist": "rimraf dist",
        "prebuild": "run-s lint remove:dist",
        "build": "webpack --mode=production",
        "prestart:dev": "rimraf build",
        "start:dev": "webpack --mode=development -w -d false",
        "linux:dev": "export NODE_ENV=\"development\" && npm run start:dev",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix"
    },
    "author": "Synogun",
    "license": "MIT",
    "dependencies": {
        "jquery": "3.7.1"
    },
    "devDependencies": {
        "@babel/core": "7.28.0",
        "@babel/preset-env": "7.27.2",
        "@eslint/js": "9.29.0",
        "@stylistic/eslint-plugin": "4.4.1",
        "@types/jquery": "3.5.32",
        "babel-loader": "10.0.0",
        "eslint": "9.31.0",
        "globals": "16.3.0",
        "npm-run-all2": "8.0.4",
        "rimraf": "6.0.1",
        "vitest": "3.2.4",
        "webpack": "5.91.0",
        "webpack-cli": "5.1.4",
        "webpack-userscript": "3.2.3"
    },
    "userscript": {
        "name": "FarmRPG Plus",
        "namespace": "Violentmonkey Scripts",
        "match": "https://farmrpg.com/*",
        "grant": [
            "none"
        ],
        "icon": "https://farmrpg.com/favicon.ico"
    }
}
